[user]
  name = Jason Bellamy
  email = j@sonbellamy.com
[core]
  editor = vim
[github]
  user = jasonbellamy
[color]
  ui = auto
[color "status"]
  added = 23
  changed = 255
  untracked = 1
[alias]
  br = branch
  co = checkout
  st = status
  lg = log -p
  cl = "!git branch --merged | grep -v '\\*' | xargs -n 1 git branch -d"
  cr = remote update --prune
[push]
  default = simple
